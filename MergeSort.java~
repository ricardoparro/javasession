class MergeSort{

    public static void main(String[] args){

    }
    //recursive merge sort method
    public static void mergeSort(int[] arr, int start, int end){
	
	if(start < end){
	    int mid = (start+ end)/2;
	    mergeSort(arr, start, mid);
	    mergeSort(arr, mid + 1 , end);
	    mergeArray(arr,start, mid, end);
	}
    }

}